{
  "expo": {
    "name": "FirstProject",
    "slug": "FirstProject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app//assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff",
      
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.govind.mit.FirstProject"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.govind.mit.FirstProject"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    
  }
}
